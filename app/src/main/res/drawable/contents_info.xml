<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="-2dp"
        android:top="-2dp"
        android:right="1dp"
        android:left="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:color="#f39801"
                android:width="1dp"/>
            <solid
                android:color="@android:color/transparent"/>
